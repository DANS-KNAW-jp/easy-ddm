<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="nl.knaw.dans.easy.domain.dataset.item.FolderItemCreatorRole" table="easy_folder_creator" schema="public">
        <id name="id" type="long">
            <column name="id" />
            <generator class="sequence">
				<param name="sequence">easy_folder_creator_id_seq</param>
            </generator>
        </id>
        <property name="folderSid" type="string">
            <column name="pid" length="64" />
        </property>
        <property name="creatorRole" type="nl.knaw.dans.easy.fedora.db.CreatorRoleEnumUserType">
            <column name="creator" length="32" not-null="true" />
        </property>
    </class>
    
</hibernate-mapping>
